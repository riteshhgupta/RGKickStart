{
  "name": "RGListKit",
  "version": "1.0",
  "summary": "RGListKit - the missing gap between UITableView & UICollectionView!",
  "authors": {
    "Ritesh Gupta": "rg.riteshh@gmail.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/riteshhgupta/RGListKit",
  "source": {
    "git": "https://github.com/riteshhgupta/RGListKit.git",
    "tag": "1.0"
  },
  "platforms": {
    "ios": "9.0"
  },
  "requires_arc": true,
  "default_subspecs": "Core",
  "subspecs": [
    {
      "name": "Core",
      "dependencies": {
        "ProtoKit": [
          "~> 1.3"
        ]
      },
      "source_files": [
        "Source/**/*.swift",
        "Source/**/*.xib"
      ],
      "exclude_files": "Source/Reactive/*.swift"
    },
    {
      "name": "ReactiveSwift",
      "dependencies": {
        "RGListKit/Core": [

        ],
        "ReactiveSwift": [
          "~> 4.0"
        ],
        "ReactiveCocoa": [
          "~> 8.0"
        ]
      },
      "source_files": "Source/**/*.swift"
    }
  ]
}
