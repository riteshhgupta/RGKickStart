# platform :ios, '9.0'

target 'RGKickStart' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

	pod 'RGListKit/ReactiveSwift', :git => 'https://github.com/riteshhgupta/RGListKit.git'
	pod 'RGNetworkKit/ReactiveSwift', :git => 'https://github.com/riteshhgupta/RGNetworkKit.git'
	pod 'RGRoutable/ReactiveSwift', :git => 'https://github.com/riteshhgupta/RGRoutable.git'
	pod 'RGDependencyInjector', :git => 'https://github.com/riteshhgupta/RGDependencyInjector.git'
	pod 'RGStoreKit', :git => 'https://github.com/riteshhgupta/RGStoreKit.git'
		
	pod 'Nuke', '~> 6.0'
	pod 'KeychainAccess'
	pod 'SwiftyUserDefaults', '4.0.0-alpha.1'
	pod 'SVProgressHUD'
end
