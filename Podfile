# Uncomment the next line to define a global platform for your project
platform :ios, '11.0'

target 'RGKickStart' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

	pod 'RGListKit/ReactiveSwift', :git => 'https://github.com/riteshhgupta/RGListKit.git', :branch => 'swift4.2'
	pod 'RGNetworkKit/ReactiveSwift', :git => 'https://github.com/riteshhgupta/RGNetworkKit.git', :branch => 'swift4.2'
	pod 'RGRoutable/ReactiveSwift', :git => 'https://github.com/riteshhgupta/RGRoutable.git', :branch => 'swift4.2'
	pod 'RGDependencyInjector', :git => 'https://github.com/riteshhgupta/RGDependencyInjector.git', :branch => 'swift4.2'
	pod 'RGStoreKit', :git => 'https://github.com/riteshhgupta/RGStoreKit.git', :branch => 'swift4.2'

	pod 'Nuke'
	pod 'KeychainAccess'
	pod 'SwiftyUserDefaults', '4.0.0-alpha.1'
	pod 'SVProgressHUD'
end
